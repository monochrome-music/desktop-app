{
  "productName": "Monochrome",
  "version": "0.1.0",
  "identifier": "com.monochrome.music",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devUrl": "https://monochrome.samidy.com",
    "frontendDist": "dist"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/icon.png",
      "icons/icon.ico"
    ],
    "iOS": {
      "minimumSystemVersion": "16.0"
    }
  },
  "app": {
    "withGlobalTauri": true,
    "security": {
      "csp": null,
      "capabilities": [
        "main-capability",
        "desktop-capability",
        "mobile-capability"
      ]
    }
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}
